{
  "parser": "typescript-eslint-parser", 
  "env": {
      "node": true,
      "es6": true
  },
  "rules": {
    "consistent-return": 0,
    "no-cond-assign": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-constant-condition": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "eqeqeq": 2,
    "curly": 2,
    "default-case": 2,
    "no-alert": 1,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 0,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "wrap-iife": [2, "outside"],
    "yoda": 2,
    "init-declarations": [0, "always"],
    "no-delete-var": 2,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-unused-vars": [0, { "vars": "all", "args": "after-used" }],
    "block-spacing": [2, "always"],
    "brace-style": 2,
    "comma-style": [2, "last"],
    "func-names": 2,
    "func-style": [2, "declaration", { "allowArrowFunctions": true }],
    "indent": ["error", "tab"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "new-cap": 0,
    "new-parens": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-new-object": 2,
    "no-spaced-func": 2,
    "keyword-spacing": ["error", { "after": true , "before": true}],
    "space-before-blocks": 2,
    "space-unary-ops": 2,
    "spaced-comment": [2, "always"],
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2
  }
}